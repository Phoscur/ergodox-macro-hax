{
  "name": "oryx-macro-hax",
  "version": "2.0.0",
  "description": "Oryx Macro Post-Processor",
  "scripts": {
    "start": "ts-node -r dotenv/config src/process.ts",
    "get": "ts-node -r dotenv/config src/download.ts",
    "test": "jest",
    "lint": "eslint",
    "tsc": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "copy": "cpy ./layout_src/**/keymap.c ./layout_src/**/config.h ./layout_src/**/rules.mk ../keyboards/moonlander/keymaps/hacked",
    "compile": "cd .. && qmk compile -kb moonlander -km hacked"
  },
  "author": "Phoscur & gittyeric",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "cpy-cli": "^4.2.0",
    "dotenv": "^16.0.3",
    "node-persist": "^3.1.3",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.1",
    "unzipper": "^0.10.11",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^18.16.6",
    "@types/node-fetch": "^2.6.3",
    "@types/node-persist": "^3.1.3",
    "@types/prompts": "^1.2.0",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
